<!DOCTYPE aesl-source>
<network>


<!--list of global events-->
<event size="0" name="start"/>
<event size="0" name="stop"/>


<!--list of constants-->


<!--show keywords state-->
<keywords flag="true"/>


<!--node e-puck0-->
<node nodeId="1" name="e-puck0">var i 

onevent start
i = 0
while i &lt; 8 do
	leds[i] = 1
	i = i + 1
end

onevent stop
i = 0
while i &lt; 8 do
	leds[i] = 0
	i = i + 1
end</node>


</network>
